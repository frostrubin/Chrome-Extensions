{
  "manifest_version": 2,
  "name": "S3 Console Simplifier",
  "description": "Simplify the S3 Console",
  "version": "0.1",
  "permissions": [
    "https://console.aws.amazon.com/s3/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://console.aws.amazon.com/s3/*"],
      "js": ["simplifier.js"],
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": { 
     "16": "icons/016.png",
     "48": "icons/048.png",
    "128": "icons/128.png",
    "904": "icons/904.png"
  }
}



